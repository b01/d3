{
	"name": "kshabazz/diablo-3-assistant",
	"description": "A tool to help play improve your game-play.",
	"require": {
		"php": ">=5.4.0",
		"kshabazz/slib": "dev-master",
		"twig/twig": "1.*",
		"pear-pear.php.net/HTML_Template_Sigma": "*",
		"guzzlehttp/guzzle": "~4.0",
		"orno/config": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit-mock-objects": "2.0.*",
		"phpunit/php-code-coverage": "2.0.*",
		"phpunit/phpunit": "3.7.*",
		"squizlabs/php_codesniffer": "1.5.*",
		"php-vcr/php-vcr": "dev-master",
		"php-vcr/phpunit-testlistener-vcr": "dev-master"
	},
	"autoload": {
		"psr-0": { "kshabazz\\": "src/" },
		"files": [
			"src/kshabazz/d3a/_Constant.php",
			"src/kshabazz/d3a/_Tool.php"
		]
	},
	"type": "tool",
	"keywords": ["diablo 3"],
	"homepage": "http://d3.kshabazz.net",
	"authors": [
		{
			"name": "Khalifah Shabazz",
			"homepage": "http://www.kshabazz.net",
			"role": "Developer"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/b01/slib.git"
		},
		{
			"type": "pear",
			"url": "http://pear.php.net"
		}
	]
}
