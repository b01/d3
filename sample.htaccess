Options +SymLinksIfOwnerMatch
RewriteEngine On
# If it is an image
# Check if an image exist on the server, if not then request it from battle.net
RewriteCond %{REQUEST_URI}			(.png|.jpg|.gif)$   [NC]
RewriteCond %{REQUEST_FILENAME}			!-f
# then rewrite as a battle.net URL and forward it to the image processor
RewriteRule ^/?media/images/(.+)(\.png|\.jpg|\.gif)$	/image-processor.php?file=%{REQUEST_URI} [NC,QSA,L]